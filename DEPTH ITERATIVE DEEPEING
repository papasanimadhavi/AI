gr={'A':['B','C'],'B':['D','E'],'C':['F','G']}

def dls(s,t,d):
  if s==t: return True
  if d<=0: return False
  for i in gr[s]:
    print(i)
    if(dls(i,t,d-1)):
      return True
  return False

def iterative(s,t,d):
  for i in range(d):
    print(i+1)
    print(s)
    if(dls(s,t,i)):
      return True
  return False


if(iterative('A','C',2)):
  print("found")
else:
  print("not found")
